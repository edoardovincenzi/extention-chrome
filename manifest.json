{
  "name": "Element info",
  "version": "1.0.0",
  "description": "Give you some information about element in hover.",
  "manifest_version": 3,
  "author": "Edoardo Vincenzi",
  "action": {
    "default_popup": "index.html",
    "default_title": "Element info"
  },
  "permission": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.google.com/*"],
      "js": ["contents.js"]
    }
  ]
}
